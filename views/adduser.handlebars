<h1>search users</h1>
  {{#if error}}<span>{{error}}</span>{{/if}}
  <form method="POST" action="/user/add">
    <div class="form-group">
      <label>userId</label>
      <input type="text" name="userId" class="form-control" placeholder="Add userId"
    </div>
    <div class="form-group">
      <label>displayName</label>
      <input type="text" name="displayName" class="form-control" placeholder="displayName"
    </div>
    <div class="form-group">
      <label>Last pictureUrl</label>
      <input type="text" name="pictureUrl" class="form-control" placeholder="pictureUrl"
    </div>
    <div class="form-group">
      <label>statusMessage</label>
      <input type="text" name="statusMessage" class="form-control" placeholder="v"
    </div>
    <input type="submit" name="save" class="btn btn-primary" value="save" />
  </form>
